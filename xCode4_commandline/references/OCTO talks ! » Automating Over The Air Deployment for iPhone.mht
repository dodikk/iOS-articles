/* codebox */
.wp_codebox {
	color: #100;
	width: 98%;
	background-color: #f9f9f9;
	border: 1px solid silver;
	margin: 0 0 .5em 0;
	overflow: auto;
}

.wp_codebox table {
	border-collapse: collapse;
	border: none;
	margin: 0px !important;
	width: 100%;
}

.wp_codebox div,.wp_codebox td {
	vertical-align: top;
	padding: 2px 4px;
}

.wp_codebox td.line_numbers {
	text-align: right;
	background-color: #def;
	color: #666;
	overflow: visible;
	border-right: 1px solid #B0BEC7;
	table-layout: auto;
	width: 15px;
}

/* potential overrides for other styles */
.wp_codebox pre {
	border: none;
	background: none;
	margin: 0;
	padding: 0;
	width: auto;
	float: none;
	clear: none;
	overflow: visible;
	font-size: 12px;
	line-height: 1.333;
	text-align:left;
}

.line_numbers pre {
	padding-left: 10px;
	text-align: right;
}

